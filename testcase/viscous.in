[solver]
max_step = 100000
monitor_step = 200
output_step = 10000

[meshfile]
file = ./grid_file.in

[flow]
type = 2
rho = 1.225
u = 2.8925e+2
v = 0
p = 1.01325e+5
gamma = 1.4
Pr = 0.72
R = 287
mu = 7.08662e-2

[reconstruct]
method = least-square
use_lim = 1

[flux]
method = lax-friedrichs

[time]
dt = 1e-4
use_cfl = 0
CFL = 1
method = explicit
rk_steps = 2
local_dt = 0
