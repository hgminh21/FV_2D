[solver]
max_step = 100000
monitor_step = 500
output_step = 5000

[meshfile]
file = ../grid_file.in

[flow]
type = 2
rho = 1.225
u = 170.1470
v = 0
p = 101325
gamma = 1.4
Pr = 0.72
R = 287
mu = 4.1686e-2

[reconstruct]
method = least-square
use_lim = nolim

[flux]
method = lax-friedrichs

[time]
dt = 1e-4
use_cfl = 1
CFL = 1
method = explicit
rk_steps = 2
local_dt = 1