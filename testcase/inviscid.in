[solver]
max_step = 100000
monitor_step = 500
output_step = 2500

[meshfile]
file = ./grid_file.in

[flow]
type = 1
rho = 1.4
u = 0.85
v = 0
p = 1
gamma = 1.4

[reconstruct]
method = gauss-green
use_lim = 1

[flux]
method = rusanov

[time]
dt = 1e-4
use_cfl = 1
CFL = 1
method = explicit
rk_steps = 2
local_dt = 1