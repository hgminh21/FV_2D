[solver]
max_step = 30000
monitor_step = 100
output_step = 2000

[meshfile]
file = ../../grid_file.in

[flow]
type = 1
rho = 1.4
u = 0.849871
v = 0.014835
p = 1
gamma = 1.4

[reconstruct]
method = green-gauss
use_lim = vanleer

[flux]
method = lax-friedrichs

[time]
dt = 1e-4
use_cfl = 1
CFL = 1
method = explicit
rk_steps = 2
local_dt = 1